{
  "manifest_version": 3,
  "name": "Custom CSS Injector",
  "version": "1.0",
  "description": "Inject custom CSS into the current page",
  "permissions": ["activeTab", "scripting", "storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "web_accessible_resources": [{
    "resources": ["lib/codemirror/*"],
    "matches": ["<all_urls>"]
  }]
} 